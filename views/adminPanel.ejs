<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Admin Panel - User List</title>
    <link rel="stylesheet" href="/public/styles.css">
</head>
<body>
    <h1>Admin Panel - User List</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>Email</th>
                <th>Role</th>
                <th>Created At</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.username %></td>
                    <td><%= user.email %></td>
                    <td><%= user.role %></td>
                    <td><%= user.created_at %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <a href="/">Back to Home</a>
</body>
</html>
